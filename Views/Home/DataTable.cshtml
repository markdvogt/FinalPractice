@{
    ViewData["Title"] = "Table";
}

@model List<FormResponse>

<table class="table table-bordered table-striped">
    <thead class="font-weight-bold">
        <tr>
            <td>Title</td>
            <td>Year</td>
            <td>Director</td>
            <td>Rating</td>
            <td>Edited</td>
            <td>Lent To</td>
            <td>Notes</td>
            <td></td>
            <td></td>
        </tr>
    </thead>

    <tbody>
        @foreach (var x in Model) //This line gives me an error-->
        {
            <tr>
                <td>@x.Title</td>
                <td>@x.Year</td>
                <td>@x.Director</td>
                <td>@x.Rating</td>
                <td>@x.Edit</td>
                <td>@x.Lent</td>
                <td>@x.Notes</td>
                
                <td><a asp-controller="Home" asp-action="Edit" asp-route-formid="@x.FormId" class="btn btn-warning">Edit</a>

                <td><a asp-controller="Home" asp-action="Delete" asp-route-formid="@x.FormId" class="btn btn-danger">Delete</a></td>

            </tr>
        }
    </tbody>

</table>